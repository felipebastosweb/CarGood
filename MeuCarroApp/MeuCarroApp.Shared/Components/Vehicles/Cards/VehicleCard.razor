@using MeuCarroApp.Shared.Components.Core.Entities
@using MeuCarroApp.Shared.Components.Vehicles.Models

<div class="card">
    <header class="card-header">
        @vehicle!.Name
    </header>
    <article class="card-body">
        <h5 class="card-title">@vehicle!.Owner!.Person!.Name</h5>
        <h6 class="card-subtitle mb-2 text-body-secondary">@vehicle!.Model!.Title</h6>
        <p class="card-text">Content</p>
        <a href="#" class="btn btn-primary">Card link</a>
        <a href="#" class="card-link">Card link</a>
    </article>
    <footer class="card-footer">

    </footer>
</div>

@code {
    public Vehicle? vehicle { get; set; }

}
